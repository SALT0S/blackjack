{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["airbnb-base", "prettier"],
	"parserOptions": {
		"ecmaVersion": 12
	},
	"rules": {
		"quotes": "warn",
		"one-var": "off",
		"no-unused-vars": "warn",
		"prefer-const": "warn",
		"no-console": "off",
		// "func-names": "off",
		"no-process-exit": "off",
		"object-shorthand": "off"
		// "class-methods-use-this": "off"
	}
}
